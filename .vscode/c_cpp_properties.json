{
    "version": 4,
    "configurations": [
        {
            "name": "gcc",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "compilerPath": "${env:MBED_GCC_ARM_PATH}/arm-none-eabi-g++",
            "forcedInclude": [
                //"Drivers/CMSIS/Device/ST/STM32U5xx/Include/stm32u5xx.h"
            ],
            "includePath": [
                "${workspaceFolder}",
                "Core/**",
                "Drivers/**",
                "FileX/**",
                "USBX/**",
                "Middlewares/**",
                "AZURE_RTOS/**"
            ],
            "defines": [
                "DEBUG",
                "STM32U5A5xx",
                "USE_HAL_DRIVER",
                "USE_FULL_LL_DRIVER",
                "TX_SINGLE_MODE_NON_SECURE=1",
                "TX_INCLUDE_USER_DEFINE_FILE",
                "FX_INCLUDE_USER_DEFINE_FILE",
                "UX_INCLUDE_USER_DEFINE_FILE",
                "TRACE_LEVEL=5"
            ]
        }
    ]
}